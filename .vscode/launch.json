{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Run API (uvicorn)",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "API.app:app",
        "--reload",
        "--host",
        "127.0.0.1",
        "--port",
        "24801"
      ],
      "env": {
        "DINOAIR_ENV": "dev",
        "DINOAIR_EXPOSE_OPENAPI_IN_DEV": "1"
      },
      "jinja": true,
      "console": "integratedTerminal"
    }
  ]
}
